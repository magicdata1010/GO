 package main

func traverse(ar ...[]int) []int {

	for i := 0; i < len(ar); i++ {
		println(ar[i], "kk")

	}
	return ar
}

func main() {

	add := func(a, b int) int {
		return a + b

	}
	{
		println(add(2, 3))
	}
	s := traverse(1)
	println(s)
}

